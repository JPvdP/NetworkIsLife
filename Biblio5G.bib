@article{Ozdemir200969,
	abstract = {This paper uses the P-star model to explain inflation dynamics in Turkey. In P-star models, money determines the price gap, which is postulated to measure the pressure on prices in an economy. This pressure emerges when output is above the potential, the interest rate is lower than the natural rate, or there is pure excess money in the economy. The estimation results with the Turkish data show that the price gap contains considerable information for explaining inflation dynamics. Moreover, the model selection criterion that compares the empirical performance of the P-star model with the new classical Phillips curve relation favors the P-star model over the Phillips curve relationship. We conclude that money is efficacious in predicting risk in price stability in Turkey. Copyright {\copyright} 2009 M.E. Sharpe, Inc.},
